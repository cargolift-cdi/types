{
  "name": "@cargolift-cdi/types",
  "version": "0.1.73",
  "description": "TypeScript types e interfaces comuns para projetos Cargolift CDI",
  "keywords": [
    "typescript",
    "types",
    "interfaces",
    "nest",
    "nestjs",
    "node"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cargolift-cdi/types.git"
  },
  "bugs": {
    "url": "https://github.com/cargolift-cdi/types/issues"
  },
  "homepage": "https://github.com/cargolift-cdi/types#readme",
  "sideEffects": false,
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
    "build": "npm run clean && tsc -p tsconfig.json",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.8",
    "@nestjs/typeorm": "^11.0.0",
    "typeorm": "^0.3.27"
  }
}
